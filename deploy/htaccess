AddHandler fastcgi-script .fcgi
Options +ExecCGI

# RewriteBase /senseicrowd
RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f [NC]
RewriteCond %{HTTP_HOST} ^senseicrowd.kozelek.cz$ [NC]
RewriteRule ^(.*)$ /honey.fcgi/$1 [QSA,L]

RewriteCond %{REQUEST_FILENAME} !-f [NC]
RewriteCond %{HTTP_HOST} !^senseicrowd.kozelek.cz$ [NC]
RewriteRule ^(.*)$ http://senseicrowd.kozelek.cz/$1 [R=301,L]

